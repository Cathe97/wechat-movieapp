/**app.wxss**/
page{
  background-color: #f9f9f9
}

.unlogin-card{
  display: flex;
  align-items: center;
  background: #ffffff;
  margin: 200rpx 20rpx 0;
  border:1px solid rgba(150, 150, 150, 0.5);
  border-radius:10rpx;
  height: 200rpx;
  }

.unlogin-head{
  flex-shrink: 0;
  margin-left: 80rpx;
  height: 100rpx;
  width: 100rpx;
  background: gainsboro;
  border-radius: 50%;
}

.unlogin-info{
  flex: 1;
  margin-left: 30rpx;
}

.unlogin-text{
  line-height: 48rpx;
  font-size:34rpx;
  color: rgba(30, 30, 30, 0.8);
  font-weight: bold;
}

.unlogin-tips{
  margin-top: 6rpx;
  line-height: 33rpx;
  font-size: 24rpx;
  color: #888888
}

.unlogin-btn{
   margin: 34rpx auto 0;
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #f9f9f9;
  border-radius: 10rpx;
  font-size: 30rpx;
  text-align: center;
  color: #34373D;
  font-weight: bold;
}